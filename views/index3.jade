		<script src='assets/js/parallax.js-1.3.1/parallax.min.js'></script>
		<link href="assets/css/parallax.css" rel="stylesheet" type="text/css"/>

		<script>
			$( document ).ready(function() {
				$('.parallax-window1').parallax({imageSrc: 'assets/img/science.jpg'});	
